{
  "singleQuote": true,
  "jsxSingleQuote": true,
  "tabWidth": 2,
  "semi": false,
  "jsxBracketSameLine": true,
  "importOrder": [
    "^react",
    "^!@",
    "^@/",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "trailingComma": "none",
  "experimentalBabelParserPluginsList": [
    "classProperties",
    "jsx"
  ]
}