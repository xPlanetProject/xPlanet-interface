{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      // Allows for the parsing of JSX
      "jsx": true
    }
  },
  "ignorePatterns": [
    "node_modules/**/*",
    "./src/usdc-price/**.*"
  ],
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "extends": [
  ],
  "rules": {
    "@typescript-eslint/explicit-function-return-type": "off",
    "prettier/prettier": "off",
    "@typescript-eslint/no-explicit-any": "off"
  }
}